{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": false,
    "strict": true,
    "moduleResolution": "node",
    "outDir": ".server",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    },
   
  },
  "include": ["server/**/*"],
}
